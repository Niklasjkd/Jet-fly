<div class="container">
  <h1>All users</h1>
  <ul>
    <% @users.each do |user| %>
    <li>
      <h2><%= link_to user.full_name, user_path(user) %></h2>
      <p><%= user.email %></p>
    </li>
    <% end %>
  </ul>
  <%= link_to "Create new user", new_user_session_path, class: 'btn btn-primary' %>
</div>
